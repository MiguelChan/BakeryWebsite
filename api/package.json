    "@babel/preset-typescript": "^7.13.0",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "babel-jest": "^26.6.3",
